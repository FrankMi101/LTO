    Public Shared Sub SetListClickgetValue2(ByRef cPage As Page, ByVal pageID As String, ByVal myText As Object, ByVal myList As CheckBoxList, ByVal _CompentencyID As String)
        Dim _TextID As String = pageID & myText.ID
        Dim _listID As String = pageID & myList.ID

        ' Dim myList As CheckBoxList = Me.chbCompetency
        Dim index As Integer = 0
        Dim chbControl As String = myList.ClientID.ToString()
        Dim _chkItem As String = ""
        Dim _chkCode As String = ""
        Dim item As ListItem
        For Each item In myList.Items
            _chkItem = chbControl & "_" & index.ToString
            _chkCode = item.Value
            item.Attributes.Add("onclick", String.Format("javascript:getCompentencyToText('" & _TextID & "','" & _chkItem & "','" & _chkCode & "')"))
            index = index + 1
            If _chkCode = _CompentencyID Then
                item.Text = "<b>" + item.Text + "</b>"
            Else
                item.Text = Replace(item.Text, "<b>", "")
                item.Text = Replace(item.Text, "</b>", "")
            End If
        Next
    End Sub
