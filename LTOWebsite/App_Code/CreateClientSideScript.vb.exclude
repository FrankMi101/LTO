    Public Shared Sub SetListItemsLinkMouseOver(ByRef cPage As Page, ByVal pageID As String, ByVal myObj As Object)

        Dim index As Integer = 0
        Dim chbControl As String = myObj.ClientID.ToString()
        Dim _chkItem As String = ""
        Dim _overCSS As String = "LinkMouseOver"
        Dim _outCSS As String = "LinkNormal"
        Dim item As ListItem
        For Each item In myObj.Items
            _chkItem = chbControl & "_" & index.ToString
            index = index + 1
            item.Attributes.Add("onmouseover", String.Format("javascript:setLink('" & _chkItem & "','" & _overCSS & "')"))
            item.Attributes.Add("onmouseout", String.Format("javascript:setLink('" & _chkItem & "','" & _outCSS & "')"))
        Next
        'myObj.Attributes.Add("onmouseover", String.Format("javascript:setLink(" & myObj & ",'" & _cSSover & "')"))
        'myObj.Attributes.Add("onmouseout", String.Format("javascript:setLink(" & myObj & ",'" & _cSSout & "')"))

    End Sub
