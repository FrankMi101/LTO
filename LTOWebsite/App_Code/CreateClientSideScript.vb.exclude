            item.Attributes.Add("onclick", String.Format("javascript:getCompentencyToText0('" & _chkCode & "','" & _chkItem & "')"))
            If item.Value = _CompentencyID Then
                item.Text = "<FONT color='#3300ff' >" + item.Text + " </FONT>"
                Dim _seleCompetency2 = HttpContext.Current.Session("SelectComtency2")
                If _seleCompetency2 = "1" Then
                    item.Text = Replace(item.Text, "src='../Images/Face_P.gif' border='0'", "src='../Images/Face_P.gif' border='2'")
                    '  item.Text = Replace(item.Text, "src='../Images/Face_P.gif' border='0'", "src='../Images/Face_P.gif' border='2'")
                    item.Text = Replace(item.Text, "border='1'", "border='0'")
                    item.Text = Replace(item.Text, "Retrieve Satisfactory Competency", "Satisfactory Competency Selected")
                    item.Text = Replace(item.Text, "Unsatisfactory Competency Selected", "Retrieve Unsatisfactory Competency")
                    item.Text = Replace(item.Text, "src='../Images/Face_N.gif' border='2'", "src='../Images/Face_N.gif' border='0'")
                End If
                If _seleCompetency2 = "2" Then
                    item.Text = Replace(item.Text, "src='../Images/Face_N.gif' border='0'", "src='../Images/Face_N.gif' border='2'")
                    ' item.Text = Replace(item.Text, "src='../Images/Face_N.gif' border='0'", "src='../Images/Face_N.gif' border='2'")
                    item.Text = Replace(item.Text, "border='1'", "border='0'")
                    item.Text = Replace(item.Text, "Satisfactory Competency Selected", "Retrieve Satisfactory Competency")
                    item.Text = Replace(item.Text, "Retrieve Unsatisfactory Competency", "Unsatisfactory Competency Selected")
                    item.Text = Replace(item.Text, "src='../Images/Face_P.gif' border='2'", "src='../Images/Face_P.gif' border='0'")
                End If

                ' item.Text = Replace(item.Text, "border='0'", "border='2'")
            Else
                item.Text = Replace(item.Text, "<FONT color='#3300ff' >", "")
                item.Text = Replace(item.Text, "</FONT>", "")
                item.Text = Replace(item.Text, "border='2'", "border='1'")
            End If
            index = index + 1
        Next



    End Sub

